{
  "name": "codex-autocommit",
  "displayName": "Codex Autocommit",
  "description": "Automatically commit and push dirty files on an interval.",
  "version": "0.0.1",
  "publisher": "nephlm",
  "repository": "https://github.com/nephlm/codex-autocommit.git",
  "engines": {
    "vscode": "^1.69.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:codex-autocommit.helloWorld",
    "onCommand:codex-autocommit.commitAndPush"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "codex-autocommit.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "codex-autocommit.commitAndPush",
        "title": "SCM Auto Commit And Push"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3",
    "@vscode/test-electron": "^2.2.0"
  }
}